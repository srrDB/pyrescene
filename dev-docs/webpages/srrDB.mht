	font-family: Verdana, Geneva, Tahoma, sans-serif;
	font-size: 11px;
	padding: 0px;
	margin: 0px;
}
a {
	font-weight: bold;
	color: #000000;
	text-decoration: none;
}
a:hover {
	text-decoration: underline;
}
ul {
	display: table;
	list-style: none;
	margin: 0px;
	padding: 0px;
	width: 100%;
}
a img {
	border: 0px;
}
form p {
	display: inline;
}
table {
	width: 100%;
	border-collapse: collapse;
	border: 1px black solid;
	background-color: #FFFFFF;
}
.highlight tr:hover {
	background-color: #99CCFF;
}
td {
	vertical-align: top;
	border-bottom: 1px #000000 solid;
}
input, textarea {
	border: 1px #000000 solid;
}
h1, h2, h3, h4, h5, h6 {
	margin: 0px;
}
#logo h1 {
	font-size: 22px;
	margin-top: -2px;
}
.list {
	border-bottom: 0px;
}
.first {
	border-top: 1px #000000 solid;
}
.crcField {
	text-align: right;
	font-family: "Lucida Console", "Lucida Sans", "Lucida Sans Regular", "Lucida Grande", "Lucida Sans Unicode", sans-serif;
}
.container {
	border: 2px #000000 solid;
	border-radius: 5px;
	-moz-border-radius: 5px;
	background-color: #CCCCCC;
	padding: 7px;
	margin-bottom: 6px;
}
.sidebyside {
	vertical-align: middle;
	float: left;
	width: 289px;
	height: 25px;
}
.half {
	width: 441px;
}
.rightalign {
	text-align: right;
	clear: right;
}
.centeralign {
	text-align: center;
}
.high {
	background-color: #FF9999;
}
.midhigh {
	background-color: #FF9966;
}
.medium {
	background-color: #FFFF66;
}
.low {
	background-color: #99FF99;
}
.error {
	color: #FF0000;
	font-weight: bold;
}
.oflow {
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
	position: absolute;
	width: 570px;
}
.login {
	font-size: 9px;
	width: 70px;
}
#site {
	width: 900px;
	margin: 0px auto;
	display: table;
}
#topBar {
	padding: 5px;
	border-top: 0px;
	margin-top: -4px;
	padding-top: 7px;
}
#search {
	width: 300px;
}
#left {
	width: 160px;
	float: left;
	margin-right: 6px;
}
#right {
	width: 734px;
	float: left;
}
#top {
	background-image: url('gfx/logo.gif');
	background-repeat: no-repeat;
	background-position: 100px 13px;
	height: 25px;
}
#main {
	clear: left;
}
#logo {
	width: 264px;
}
#topmenu {
	padding-top: 7px;
}
#topmenu li {
	float: left;
	margin-left: 15px;
	text-align: center;
}
#topsearch {
	width: 328px;
}
/*details.php*/
#warningsicon {
	width: 9px; 
	height: 9px;
	float: left; 
	background-image: url('gfx/plusminus.gif');
	background-position: 9px 0px;
}
.extension {
	background-image: url('gfx/extensions.png');
	float: left;
	width: 16px;
	height: 16px;
}
.jpg {
	background-position: -16px 0px;
}
.nfo {
	background-position: -32px 0px;
}
.txt {
	background-position: -48px 0px;
}
.sfv {
	background-position: -64px 0px;
}
.png {
	background-position: -80px 0px;
}
.srs {
	background-position: -96px 0px;
}
.rar {
	background-position: -128px 0px;
}
/*index.php*/
#bars {
	position: relative;
	height: 120px;
	top: 13px;
	margin-bottom: 6px;
}
.bar {
	width: 25px;
	height: 13px;
	position: absolute;
	bottom: 13px;
	background-color: #FF9966;
	float: left;
	text-align: center;
}
.bottombar {
	background-color: #808080;
	top: 107px;
	bottom: 0px;
	width: 29px;
	color: #FFFFFF;
}
.bar:hover {
	background-color: #99CCFF;
	cursor: pointer;
}
/*icon stuff*/
.text_16 {
	float: left;
}
.text_16m, .icon_16 {
	margin-left: 4px;
	float: left;
}
.icon_16 {
	width: 16px;
	height: 16px;
}
